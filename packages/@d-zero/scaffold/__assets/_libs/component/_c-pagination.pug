nav.c-pagination(aria-label="ページ番号")
	.c-pagination__prev
		a(data-disabled="true") prev
	.c-pagination__next
		a(href="___NEXT_PAGE___" rel="next") next
	ol.c-pagination__numbers
		li.c-pagination__number
			a(aria-current="page") 1
		li.c-pagination__number
			a(href="___NEXT_PAGE___") 2
		li.c-pagination__number
			a(href="___NEXT_PAGE___") 3
		li.c-pagination__number
			a(href="___NEXT_PAGE___") 4
